{  
    "manifest_version":2,
    "name":"Twitch Control",
    "version":"0.2",
    "description":"Control Twitch.tv tabs",
    "browser_action":{  
		"default_popup": "popup.html"
    },
    "author":"Tyler Sutherland",
	"permissions":[
        "tabs",
        "http://*/",
        "https://*/"
	]
}
